
public class OrderedLinkedList<T> extends LinkedList<T> {
	
	public void addFirst(T data){
		throw new UnsupportedOperationException("addFirst is not implemented for OrderedLinkedList<T> as it contradicts the logic of an ordered list.");
	}
	
	public void addLast(T data){
		throw new UnsupportedOperationException("addLast is not implemented for OrderedLinkedList<T> as it contradicts the logic of an ordered list.");
	}
	
	public void insert(T data){
			Node<T> current = head;
			if(head == null){head = new Node<T>(data);
	}
	
}
