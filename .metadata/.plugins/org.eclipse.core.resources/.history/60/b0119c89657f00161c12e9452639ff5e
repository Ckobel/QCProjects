
public class Primes {

	public static void first100Primes() {

		for (int i = 0; i < 100; i++) {

			boolean flag = true;
			for (int j = 0; j < Math.sqrt(i); j++) {
				if (i % j == 0) {
					flag = false;
				}
				if (flag) {
					System.out.println(i);
				}
			}
		}
	}
	
	public static void main(String[] args){
		first100Primes();
	}
}
