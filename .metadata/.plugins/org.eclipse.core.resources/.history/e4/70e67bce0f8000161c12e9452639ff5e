
public class Primes {

	public static void first1000Primes() {

		for (int i = 1; i*i < 1000; i++) {

			boolean flag = true;
			for (int j = 2; j < (int) (Math.sqrt(i) + .5); j++) {
				if (i % j == 0) {
					flag = false;
				}

			}
			if (flag) {
				System.out.print(i + " ");
			}
		}
	}

	public static void main(String[] args) {
		first1000Primes();
	}
}
