public class CircularlyLinkedList<T> {

	public CircularlyLinkedList(){
		
	}
	
	public class Node<T> {
		private Node<T> next;
		private T data;

		public Node(T data) {
			next = null;
			this.data = data;
		}

		public Node(Node<T> prev, Node<T> next, T data) {
			this.data = data;
		}

		public T getData() {
			return data;
		}

		public void setNext(Node<T> next) {
			this.next = next;
		}

		public Node<T> getNext() {
			return next;
		}

	}
}
