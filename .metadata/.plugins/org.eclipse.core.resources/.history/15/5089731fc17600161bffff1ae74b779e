 public class javahelp {
public static void main(String[] args)
{

    System.out.print("| ");

    for (int j =1; j <=10; j++);
    {
        if (j <= 9)
        {
            System.out.print(j);
        }
        else
        {   
            System.out.print(0);
        }
    }
    System.out.print(" |");
}
}